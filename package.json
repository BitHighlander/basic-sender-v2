{
  "name": "pioneer-template",
  "version": "0.1.0",
  "private": true,
  "author": "highlander",
  "scripts": {
    "pnpm": "pnpm i",
    "dev": "vite",
    "build": "tsc && NODE_OPTIONS=--max_old_space_size=17000 vite build",
    "build:turbo": "npx turbo build",
    "build:serve": "pnpm build && pnpm serve",
    "serve": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc",
    "check:turbo": "npx turbo lint type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@coinmasters/types": "latest",
    "@coinmasters/wallet-evm-extensions": "latest",
    "@coinmasters/wallet-keepkey": "latest",
    "@coinmasters/wallet-keplr": "latest",
    "@coinmasters/wallet-keystore": "latest",
    "@coinmasters/wallet-ledger": "latest",
    "@coinmasters/wallet-metamask": "latest",
    "@coinmasters/wallet-okx": "latest",
    "@coinmasters/wallet-trezor": "latest",
    "@coinmasters/wallet-wc": "latest",
    "@coinmasters/wallet-xdefi": "latest",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@fontsource/plus-jakarta-sans": "^5.0.18",
    "@pioneer-platform/pioneer-coins": "^9.2.1",
    "@pioneer-sdk/pioneer-react": "0.1.1",
    "@typescript-eslint/parser": "^6.18.1",
    "buffer": "^6.0.3",
    "dotenv": "^16.3.1",
    "eslint-plugin-prettier": "4.2.1",
    "events": "^3.3.0",
    "framer-motion": "^10.18.0",
    "million": "2.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.21.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-polyfill-node": "^0.12.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.8.1",
    "@types/node": "^20.11.0",
    "@types/react": "18.2.27",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.2.1",
    "commitlint": "^17.8.1",
    "eslint": "^8.56.0",
    "eslint-config-sznm": "^2.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "typescript": "5.2.2",
    "vite": "^4.5.1",
    "vite-plugin-checker": "^0.6.2",
    "vite-tsconfig-paths": "^4.2.3"
  },
  "engines": {
    "node": ">=16.x.x",
    "pnpm": ">=8"
  }
}
